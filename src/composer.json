{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-simplexml": "*",
        "aws/aws-sdk-php": "^3.233",
        "brick/phonenumber": "^0.4.0",
        "brick/postcode": "^0.2.7",
        "dompdf/dompdf": "^2.0",
        "erusev/parsedown-extra": "^0.8.1",
        "gocardless/gocardless-pro": "^4.20",
        "guzzlehttp/guzzle": "^7.5",
        "http-interop/http-factory-guzzle": "^1.0",
        "inertiajs/inertia-laravel": "^0.5.4",
        "laravel/cashier": "^14.2",
        "laravel/framework": "^9.19",
        "laravel/passport": "^10.4",
        "laravel/sanctum": "^2.8",
        "laravel/scout": "^9.4",
        "laravel/telescope": "^4.9",
        "laravel/tinker": "^2.7",
        "league/oauth2-client": "^2.6",
        "meilisearch/meilisearch-php": "0.23.3",
        "microsoft/microsoft-graph": "^1.73",
        "moneyphp/money": "^4.0",
        "nyholm/psr7": "^1.5",
        "nyholm/psr7-server": "^1.0",
        "paragonie/constant_time_encoding": "^2.6",
        "pragmarx/google2fa": "^8.0",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "stancl/tenancy": "^3.6",
        "stripe/stripe-php": "^9.0",
        "symfony/intl": "^6.1",
        "symfony/psr-http-message-bridge": "^2.1",
        "tightenco/ziggy": "^1.0",
        "twilio/sdk": "^6.43",
        "web-auth/webauthn-lib": "^4.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.10",
        "laravel/breeze": "^1.11",
        "laravel/envoy": "^2.8",
        "laravel/sail": "^1.15",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
